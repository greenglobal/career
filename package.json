    "copy-dir": "latest",
    "css-mqpacker": "latest",
    "cssnano": "latest",
    "express": "latest",
    "handlebars": "latest",
    "html-minifier": "latest",
    "http-server": "latest",
    "imagemin": "latest",
    "imagemin-mozjpeg": "latest",
    "imagemin-pngquant": "latest",
    "minimist": "latest",
    "mkdirp": "latest",
    "postcss": "latest",
    "postcss-cssnext": "latest",
    "postcss-filter-plugins": "latest",
    "postcss-scss": "latest",
    "precss": "latest",
    "promise-wtf": "latest",
    "recursive-readdir": "latest",
    "rucksack-css": "latest",
    "shift-codegen": "latest",
    "shift-parser": "latest",
